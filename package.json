{
  "name": "cookie-consent",
  "version": "2.0.2",
  "description": "A great, simple and customizable cookie consent that automatically shows up only to the users subject to the cookie law (EU users).",
  "main": "cookie-consent.js",
  "module": "cookie-consent.js",
  "types": "cookie-consent.d.ts",
  "scripts": {
    "start": "run-p watch serve browser-sync",
    "watch": "tsc -w",
    "serve": "polymer serve --npm -p 8082",
    "browser-sync": "browser-sync start --proxy 'localhost:8082' --port 8080 --ui-port 8081 --files 'demo' 'cookie-consent.js'",
    "build": "tsc",
    "lint": "tslint -p .",
    "format": "tslint -p . --fix"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "browser-sync": "^2.26.0",
    "npm-run-all": "^4.1.3",
    "polymer-cli": "^1.8.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dabolus/cookie-consent.git"
  },
  "keywords": [
    "custom-element",
    "web-component",
    "cookie-consent",
    "cookie-law"
  ],
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dabolus/cookie-consent/issues"
  },
  "homepage": "https://github.com/Dabolus/cookie-consent#readme"
}
